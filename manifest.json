{
  "manifest_version": 3,
  "name": "MetaTFT Helper",
  "version": "1.0.0",
  "description": "Chrome extension for metatft.com",
  "permissions": ["activeTab"],
  "host_permissions": [
    "*://metatft.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://metatft.com/*"],
      "js": ["utils/adRemover.js", "content.js"],
      "type": "module"
    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}